{
  "name": "clip-download-path",
  "displayName": "Clip download path",
  "version": "0.0.1",
  "description": "clip download file path",
  "author": "172583172@qq.com",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@ant-design/icons": "5.4.0",
    "antd": "5.20.5",
    "next": "14.2.8",
    "plasmo": "0.89.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "downloads",
      "notifications",
      "scripting",
      "activeTab",
      "clipboardWrite",
      "storage"
    ],
    "host_permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/small_icon.png"
        ],
        "matches": [
          "*://*/*"
        ]
      }
    ]
  }
}